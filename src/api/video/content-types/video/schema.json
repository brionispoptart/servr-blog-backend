{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "singularName": "video",
    "pluralName": "videos",
    "displayName": "Video"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "Description": {
      "type": "richtext"
    },
    "URL": {
      "type": "string"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "videos"
    },
    "Publication_Date": {
      "type": "datetime"
    },
    "Thumbnail": {
      "type": "component",
      "repeatable": true,
      "component": "media.image"
    }
  }
}
