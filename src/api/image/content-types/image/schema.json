{
  "kind": "collectionType",
  "collectionName": "images",
  "info": {
    "singularName": "image",
    "pluralName": "images",
    "displayName": "Image"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Image_file": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "images"
    },
    "Upload_Date": {
      "type": "datetime"
    }
  }
}
